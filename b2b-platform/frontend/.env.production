# Production API Configuration
VITE_API_URL=https://api.orbiz.asia
VITE_USE_MOCK=false

# External Services
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key-here
VITE_SENTRY_DSN=your-sentry-dsn-for-error-tracking

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PWA=true
VITE_ENABLE_HOT_RELOAD=false

# App Configuration
VITE_APP_NAME="B2B Platform"
VITE_APP_VERSION="1.0.0"
VITE_DEFAULT_LOCALE=en

# Production Settings
VITE_DEV_TOOLS=false
VITE_LOG_LEVEL=error

# File Upload Settings
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_IMAGE_TYPES=image/jpeg,image/jpg,image/png,image/webp

# Social/External Links
VITE_SUPPORT_EMAIL=support@orbiz.asia
VITE_COMPANY_WEBSITE=https://orbiz.asia